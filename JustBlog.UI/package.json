{
  "name": "justblog",
  "version": "1.0.1",
  "description": "Node packages required to build the JustBlog AngularJS and .Net Core Application",
  "author": "Brian Mikinski",
  "email": "brian.mikinski@gmail.com",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/BrianMikinski/JustBlog"
  },
  "scripts": {
    "build": "webpack --config=webpack.config.js --env dev --progress --profile --colors --display verbose",
    "build:prod": "webpack --config=webpack.config.js --env prod --progress --profile --colors",
    "build:test": "webpack --config=webpack.config.js --env test --progress --profile --colors",
    "test": "karma start .\\conf\\karma.conf.js",
    "analyzeBundle": "npm run buildStats && npm run cleanStatsfile && npm run startBundleAnalyzer",
    "buildStats": "webpack --config=webpack.config.js --env dev --profile --json > stats.json",
    "buildStatsProd": "webpack --config=webpack.config.js --env prod --profile --json > stats.json",
    "buildStatsWin": "webpack --profile --json | Out-file 'dist/stats.json' -Encoding OEM (only works when run from powershell!)",
    "cleanStatsfile": "@powershell ./cleanWebpackStats.ps1 -ExecutionPolicy Unrestricted -NoProfile ",
    "startBundleAnalyzer": "webpack-bundle-analyzer stats.json"
  },
  "dependencies": {
    "@uirouter/angularjs": "1.0.22",
    "@uirouter/visualizer": "7.0.0",
    "angular": "1.8.0",
    "angular-animate": "1.7.8",
    "angular-mocks": "1.7.8",
    "angular-route": "1.7.8",
    "angular-sanitize": "1.7.8",
    "angular-ui-bootstrap": "^2.5.0",
    "angular-ui-tinymce": "0.0.19",
    "bootstrap": "3.4.1",
    "font-awesome": "^4.7.0",
    "jquery": "3.4.1",
    "tinymce": "5.6.0",
    "toastr": "^2.1.2"
  },
  "devDependencies": {
    "@types/angular": "1.6.56",
    "@types/angular-animate": "1.5.10",
    "@types/angular-mocks": "1.7.0",
    "@types/angular-sanitize": "1.7.0",
    "@types/angular-ui-bootstrap": "0.13.47",
    "@types/angular-ui-router": "1.1.40",
    "@types/bootstrap": "3.4.0",
    "@types/jasmine": "3.4.0",
    "@types/jquery": "3.3.31",
    "@types/requirejs": "2.1.31",
    "@types/tinymce": "4.5.22",
    "@types/toastr": "2.1.37",
    "@types/webpack-env": "1.14.0",
    "aspnet-webpack": "3.0.0",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "3.1.0",
    "event-stream": "3.3.4",
    "exports-loader": "0.7.0",
    "extract-loader": "3.1.0",
    "file-loader": "4.2.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-loader": "0.5.5",
    "imports-loader": "0.8.0",
    "jasmine": "3.4.0",
    "jasmine-core": "3.4.0",
    "jslint": "0.12.1",
    "karma": "4.2.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "1.1.2",
    "karma-edge-launcher": "0.4.2",
    "karma-firefox-launcher": "1.2.0",
    "karma-ie-launcher": "1.0.0",
    "karma-jasmine": "2.0.1",
    "karma-junit-reporter": "2.0.0",
    "karma-ng-html2js-preprocessor": "1.0.0",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-requirejs": "1.1.0",
    "karma-webpack": "4.0.2",
    "protractor": "6.0.0",
    "requirejs": "2.3.6",
    "style-loader": "1.0.0",
    "ts-loader": "6.0.4",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "tslint": "5.9.0",
    "typescript": "3.4.1",
    "webpack": "4.39.2",
    "webpack-bundle-analyzer": "3.4.1",
    "webpack-cli": "3.3.6",
    "webpack-dev-middleware": "3.7.0",
    "webpack-hot-middleware": "2.25.0",
    "yargs": "14.0.0"
  }
}
